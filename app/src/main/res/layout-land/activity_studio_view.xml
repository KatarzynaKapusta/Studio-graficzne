<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/studio_walls"
    tools:context=".activity_studio_view">

    <!--Generic floor -->
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/studio_floor_generic"
        tools:ignore="ContentDescription">

    </ImageView>

    <!--Office desks -->
    <ImageView
        android:id="@+id/studio_desk1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_desk2"
        app:layout_heightPercent="22%"
        app:layout_marginBottomPercent="7%"
        app:layout_marginLeftPercent="55%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="72%"
        app:layout_widthPercent="20%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_desk2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_desk2"
        android:rotation="90"
        app:layout_widthPercent="20%"
        app:layout_heightPercent="20%"
        app:layout_marginBottomPercent="10%"
        app:layout_marginLeftPercent="0%"
        app:layout_marginRightPercent="30%"
        app:layout_marginTopPercent="70%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_desk3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="90"
        android:src="@drawable/office_desk3"
        app:layout_heightPercent="18%"
        app:layout_widthPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="0%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="13%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_desk4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_desk1"
        app:layout_heightPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="35%"
        app:layout_marginRightPercent="20%"
        app:layout_marginTopPercent="75%"
        app:layout_widthPercent="18%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_desk5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_desk1"
        app:layout_heightPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="35%"
        app:layout_marginRightPercent="20%"
        app:layout_marginTopPercent="50%"
        app:layout_widthPercent="18%"
        tools:ignore="ContentDescription">

    </ImageView>


    <!--Not changeable elements -->

    <ImageView
        android:id="@+id/studio_table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_table"
        android:rotation="90"
        app:layout_heightPercent="32%"
        app:layout_widthPercent="32%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="73%"
        app:layout_marginRightPercent="0%"
        app:layout_marginTopPercent="36%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_armchair1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_armchair1"
        app:layout_heightPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="45%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="6%"
        app:layout_widthPercent="10%"
        tools:ignore="ContentDescription">

    </ImageView>



    <ImageView
        android:id="@+id/studio_armchair2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_armchair1"
        android:rotation="-90"
        app:layout_heightPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="35%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="20%"
        app:layout_widthPercent="10%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_printer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_printer"
        app:layout_heightPercent="16%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="19%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="6%"
        app:layout_widthPercent="10%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_printer2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_printer"
        android:rotation="180"
        app:layout_heightPercent="16%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="19%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="78.5%"
        app:layout_widthPercent="10%"
        tools:ignore="ContentDescription">

    </ImageView>


    <ImageView
        android:id="@+id/studio_watertank1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_watertank"
        app:layout_heightPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="65%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="6%"
        app:layout_widthPercent="10%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_watertank2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_watertank"
        app:layout_heightPercent="18%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="56%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="6%"
        app:layout_widthPercent="10%"
        tools:ignore="ContentDescription">

    </ImageView>


    <!--Plants -->

    <ImageView
        android:id="@+id/studio_plant2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="-90"
        android:src="@drawable/office_plant2"
        app:layout_heightPercent="17%"
        app:layout_widthPercent="12%"
        app:layout_marginBottomPercent="1%"
        app:layout_marginLeftPercent="81%"
        app:layout_marginRightPercent="5%"
        app:layout_marginTopPercent="80%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_plant3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="-90"
        android:src="@drawable/office_plant2"
        app:layout_heightPercent="17%"
        app:layout_widthPercent="12%"
        app:layout_marginBottomPercent="78%"
        app:layout_marginLeftPercent="81%"
        app:layout_marginRightPercent="5%"
        app:layout_marginTopPercent="5%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_plant1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="-90"
        android:src="@drawable/office_plant1"
        app:layout_heightPercent="20%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="3%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="42%"
        app:layout_widthPercent="12%"
        tools:ignore="ContentDescription">

    </ImageView>

    <ImageView
        android:id="@+id/studio_plant4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/office_plant1"
        app:layout_heightPercent="15%"
        app:layout_marginBottomPercent="5%"
        app:layout_marginLeftPercent="28%"
        app:layout_marginRightPercent="10%"
        app:layout_marginTopPercent="6%"
        app:layout_widthPercent="12%"
        tools:ignore="ContentDescription">

    </ImageView>








</androidx.percentlayout.widget.PercentRelativeLayout>